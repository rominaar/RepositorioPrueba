
public class MyComplex {
	private double real, imag;

	//Getters and setters
	public double getReal() {
		return real;
	}

	public void setReal(double real) {
		this.real = real;
	}

	public double getImag() {
		return imag;
	}

	public void setImag(double imag) {
		this.imag = imag;
	}

	//constructor
	public MyComplex(double real, double imag) {
	      this.real = real;
	      this.imag = imag;
     }
	
	
}
